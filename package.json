{
  "name": "@johanaarstein/dotlottie-player",
  "version": "1.2.37",
  "description": "dotLottie Web Component for playing vectorized Lottie animations in you web app",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js"
  },
  "homepage": "https://www.aarstein.media/en/dev/dotlottie-player",
  "repository": {
    "url": "https://github.com/johanaarstein/dotlottie-player.git",
    "type": "git"
  },
  "bugs": "https://github.com/johanaarstein/dotlottie-player/issues",
  "author": {
    "name": "Johan Martin Aarstein",
    "email": "johan@aarstein.media",
    "url": "https://www.aarstein.media",
    "organization": "Aarstein Media"
  },
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "npm run cleanup && rollup -c",
    "build:types": "tsc --declaration",
    "start": "npm run cleanup && rollup -c --watch",
    "lint": "tsc && eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "cleanup": "shx rm -rf dist && shx mkdir dist"
  },
  "dependencies": {
    "fflate": "^0.7.4",
    "lottie-web": "^5.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "eslint": "^8.30.0",
    "lit": "^2.6.1",
    "rollup": "^3.7.5",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "rollup-plugin-serve": "^2.0.0",
    "shx": "^0.3.4",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "lottie",
    "dotlottie",
    "animation",
    "web component",
    "component",
    "lit-element",
    "svg",
    "vector",
    "player"
  ],
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
