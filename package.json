{
  "name": "@johanaarstein/dotlottie-player",
  "version": "1.3.13",
  "description": "dotLottie Web Component for playing vectorized Lottie animations in your web app",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js"
  },
  "homepage": "https://www.aarstein.media/en/dev/dotlottie-player",
  "repository": {
    "url": "https://github.com/johanaarstein/dotlottie-player.git",
    "type": "git"
  },
  "bugs": "https://github.com/johanaarstein/dotlottie-player/issues",
  "author": {
    "name": "Johan Martin Aarstein",
    "email": "johan@aarstein.media",
    "url": "https://www.aarstein.media",
    "organization": "Aarstein Media"
  },
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "npm run cleanup && rollup -c && cp ./types/index.d.ts ./dist",
    "build:types": "tsc --declaration",
    "start": "npm run cleanup && rollup -c --watch",
    "lint": "tsc && eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "lint:pkg": "npmPkgJsonLint .",
    "cleanup": "shx rm -rf dist && shx mkdir dist"
  },
  "dependencies": {
    "fflate": "^0.7.4",
    "lit": "^2.6.1",
    "lottie-web": "^5.9.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@swc/core": "^1.3.27",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "npm-package-json-lint": "^6.4.0",
    "rollup": "^3.7.5",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-html-literals": "^1.1.6",
    "rollup-plugin-node-externals": "^5.1.0",
    "rollup-plugin-swc3": "^0.8.0",
    "shx": "^0.3.4",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "lottie",
    "dotlottie",
    "animation",
    "web component",
    "component",
    "lit-element",
    "svg",
    "vector",
    "player"
  ],
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
