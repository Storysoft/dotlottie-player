{
  "env": {
    "targets": "> 0.25%, not dead"
  },
  "jsc": {
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "decoratorsBeforeExport": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  },
  "module": {
    "type": "es6"
  }
}