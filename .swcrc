{
  "env": {
    "mode": "entry",
    "targets": "> 0.25%, not dead",
    "coreJs": "3.29"
  },
  "jsc": {
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "decoratorsBeforeExport": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  }
}